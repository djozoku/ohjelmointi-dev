{
  "name": "ohjelmointi-dev",
  "version": "0.0.0",
  "private": true,
  "engines": {
    "node": "10"
  },
  "scripts": {
    "dev": "next src/app",
    "preserve": "npm run build-public && npm run build-functions && npm run build-app && npm run copy-deps && npm run install-deps",
    "server": "cross-env NODE_ENV=production firebase serve",
    "deploy": "firebase deploy",
    "clean": "rimraf \"dist/functions\" && rimraf \"dist/public\"",
    "build-app": "next build \"src/app\"",
    "build-public": "cpx \"src/public/**/*.*\" \"dist/public\" -C",
    "build-functions": "tsc --project src/functions",
    "lint-app": "tslint --project src/app",
    "typecheck-app": "tsc --project src/app",
    "lint-functions": "tslint --project src/functions",
    "copy-deps": "cpx \"*{package.json,package-lock.json,yarn.lock}\" \"dist/functions\" -C",
    "install-deps": "cd \"dist/functions\" && npm i"
  },
  "dependencies": {
    "@material-ui/core": "^3.9.3",
    "@material-ui/icons": "^3.0.2",
    "@material-ui/styles": "^3.0.0-alpha.10",
    "@zeit/next-typescript": "^1.1.1",
    "apollo-cache-inmemory": "^1.5.1",
    "apollo-client": "^2.5.1",
    "apollo-link-http": "^1.5.14",
    "apollo-link-schema": "^1.2.2",
    "draft-js": "^0.10.5",
    "express": "^4.16.4",
    "firebase": "^6.0.2",
    "firebase-admin": "^7.3.0",
    "firebase-functions": "^2.3.1",
    "graphql": "^14.3.0",
    "isomorphic-unfetch": "^3.0.0",
    "jss": "^9.8.7",
    "next": "^9.3.2",
    "next-redux-saga": "^4.0.2",
    "next-redux-wrapper": "^3.0.0-alpha.2",
    "react": "^16.8.6",
    "react-apollo": "^2.5.5",
    "react-dom": "^16.8.6",
    "react-jss": "^8.6.1",
    "react-redux": "^7.0.3",
    "redux": "^4.0.1",
    "redux-devtools-extension": "^2.13.8",
    "redux-saga": "^1.0.2",
    "styled-jsx": "^3.2.1"
  },
  "devDependencies": {
    "@types/draft-js": "^0.10.32",
    "@types/express": "^4.16.1",
    "@types/material-ui": "^0.21.6",
    "@types/next": "^8.0.5",
    "@types/next-redux-saga": "^3.0.1",
    "@types/next-redux-wrapper": "^2.0.2",
    "@types/react": "^16.8.17",
    "@types/react-jss": "^8.6.3",
    "@types/react-redux": "^7.0.9",
    "@types/styled-jsx": "^2.2.8",
    "cpx": "^1.5.0",
    "cross-env": "^5.2.0",
    "firebase-functions-test": "^0.1.6",
    "firebase-tools": "6.8.0",
    "prettier": "^1.17.1",
    "rimraf": "2.6.3",
    "tslint": "^5.16.0",
    "tslint-react": "^4.0.0",
    "typescript": "^3.4.5"
  }
}
